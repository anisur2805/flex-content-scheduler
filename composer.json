{
  "name": "anisur2805/flex-content-scheduler",
  "description": "Schedule post expiry and visibility changes in WordPress",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "autoload": {
    "psr-4": {
      "Anisur\\ContentScheduler\\": "includes/"
    }
  },
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "brain/monkey": "^2.6",
    "mockery/mockery": "^1.5",
    "squizlabs/php_codesniffer": "^3.13",
    "wp-coding-standards/wpcs": "^3.3",
    "doctrine/instantiator": "^1.5"
  },
  "config": {
    "platform": {
      "php": "7.4.33"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
